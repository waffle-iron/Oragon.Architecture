<?xml version="1.0" encoding="utf-8" ?>
<objects
	xmlns="http://www.springframework.net"
	xmlns:wcf="http://www.springframework.net/wcf"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:aop="http://www.springframework.net/aop"
	xmlns:nv="http://www.springframework.net/nvelocity"
	>

	<object id="ServiceDescriptor" type="Oragon.Architecture.Services.Descriptor.ServiceDescriptor">
		<property name="Name" value="OragonLogEngine" />
		<property name="FriendlyName" value="Oragon Enterprise Log Engine" />
		<property name="Description" value="Serviço de processamento de logs responsável pelo armazenamento adequado do log" />
		<property name="StartMode" value="AutomaticallyDelayed" />
		<property name="IdentityType" value="NetworkService" />
		<!--<property name="IdentityType" value="Custom" />-->
		<property name="CustomIdentityCredential" >
			<object type="Oragon.Architecture.Security.Authentication.Credential, Oragon.Architecture.Security">
				<property name="Username" value="\LogEngineSvcUsr" />
				<property name="Password" value="LogEngine@12#" />
			</object>
		</property>
		<property name="Dependences">
			<list element-type="String">
				<!--<value>MSMQ</value>-->
			</list>
		</property>
	</object>

	<object id="ServiceManager" type="Oragon.Architecture.Services.ServiceManager, Oragon.Architecture.Services">
		<property name="StartTimeOut">
			<object type="System.TimeSpan">
				<constructor-arg name="hours" value="0" />
				<constructor-arg name="minutes" value="5" />
				<constructor-arg name="seconds" value="0" />
			</object>
		</property>
		<property name="StopTimeOut" >
			<object type="System.TimeSpan">
				<constructor-arg name="hours" value="0" />
				<constructor-arg name="minutes" value="5" />
				<constructor-arg name="seconds" value="0" />
			</object>
		</property>
	</object>


</objects>