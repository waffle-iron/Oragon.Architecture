<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<configSections>
		<sectionGroup name="spring">
			<section name="context" type="Spring.Context.Support.ContextHandler, Spring.Core"/>
			<section name="objects" type="Spring.Context.Support.DefaultSectionHandler, Spring.Core"/>
		</sectionGroup>
	</configSections>
	<spring>
		<context>
			<resource uri="config://spring/objects"/>
		</context>
		<objects xmlns="http://www.springframework.net" xmlns:wcf="http://www.springframework.net/wcf" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:aop="http://www.springframework.net/aop">

			<object name="Teste1" type="Oragon.Sample01.ConsoleApp.Pessoa, Oragon.Sample01.ConsoleApp">
				<property name="Nome" value="Luiz Carlos Faria" />
				<property name="Idade" value="29" />
				<property name="Animais">
					<list element-type="Oragon.Sample01.ConsoleApp.IAnimal, Oragon.Sample01.ConsoleApp">
						<object type="Oragon.Sample01.ConsoleApp.Cachorro, Oragon.Sample01.ConsoleApp">
							<property name="Nome" value="Ziggy" />
						</object>
						<object type="Oragon.Sample01.ConsoleApp.Calopsita, Oragon.Sample01.ConsoleApp">
							<property name="Nome" value="Tufão" />
						</object>
					</list>
				</property>
			</object>

			<object name="Teste2" type="Oragon.Sample01.ConsoleApp.Pessoa, Oragon.Sample01.ConsoleApp">

			</object>

		</objects>
	</spring>


	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
	</startup>
</configuration>