<?xml version="1.0"?>
<configuration>
	<configSections>
		<sectionGroup name="spring">
			<section name="context" type="Spring.Context.Support.ContextHandler, Spring.Core"/>
			<section name="objects" type="Spring.Context.Support.DefaultSectionHandler, Spring.Core"/>
		</sectionGroup>
	</configSections>

	<spring>
		<context>
			<resource uri="config://spring/objects"/>
		</context>
		<objects xmlns="http://www.springframework.net">
			<!--Connection Strings ######################################################################################################################### -->
			<object name="ConnectionString" type="String">
				<constructor-arg value="Provider=SQLOLEDB.1;Persist Security Info=False;Data Source=report.imusica.corp;User ID=sa;Password=zaq13edc;Connect Timeout=2000; pooling='true'; Max Pool Size=1000;Application Name=CodeGenerator" />
			</object>

			<alias name="GenerationContainer" alias="Main"/>

			<object name="BaseNamespace" type="System.String">
				<constructor-arg value="iMusica.Enterprise.Data.iMusicaImportacaoGenerica" />
			</object>

			<object name="NameStretegy" type="Oragon.CodeGen.MetaData.Plugin.iMusicaNameStretegy, Oragon.CodeGen.MetaData.Domain" >
				<property name="LanguageConvention" ref="LanguageConvention" />
				<property name="Dictionary">
					<list element-type="String">
					</list>
				</property>
				<property name="FKConvention">
					<dictionary key-type="String" value-type="String">
						<entry key="FILE.FK_METADATAFILE_FILE" value="MetadataInfo" />
						<entry key="FILE.FK_FILE_METADATA" value="OwnerMetadata" />
						
						<entry key="METADATA.FK_METADATAFILE_FILE" value="File" />
						<entry key="METADATA.FK_FILE_METADATA" value="FilesInMetadata" />
					</dictionary>
				</property>
				
			</object>

			<object name="LanguageConvention" type="Oragon.CodeGen.MetaData.Plugin.EnglishLanguageConvention, Oragon.CodeGen.MetaData.Domain" />

			<object name="MyMetaDomainConverterWrapper" type="Oragon.CodeGen.MetaData.MyMetaDomainConverterWrapper, Oragon.CodeGen.MetaData.Domain">
				<property name="DataBaseName" value="iMusicaImportacaoGenerica" />
				<property name="ConnectionString" ref="ConnectionString" />
				<property name="DbDriver" value="SQL" />
				<property name="Language"  value="C#" />
				<property name="LanguageMappingFileName" value="Languages.xml" />
			</object>
			
			<object name="GenerationContainer" type="Oragon.CodeGen.Templating.GenerationContainer, Oragon.CodeGen.Templating">
				<property name="Templates">
					<list element-type="Oragon.CodeGen.Templating.T4Template, Oragon.CodeGen.Templating">
						<!--###################################-->
						<!--Entidades-->
						<!--###################################-->
						<object type="Oragon.CodeGen.Templating.T4Template, Oragon.CodeGen.Templating">
							<property name="TemplatePath" value="..\Template.Entities.tt"/>
							<property name="OutputPath" value="..\..\iMusica.Enterprise.Data.iMusicaImportacaoGenerica\Entity\[BusinessEntity].CodeGen.cs"/>
							<property name="TemplateParameters">
								<dictionary key-type="System.String" value-type="System.Object">
									<entry key="NameSpace" value-ref="BaseNamespace" />
									<entry key="MyMetaDomainConverterWrapper" value-ref="MyMetaDomainConverterWrapper" />
								</dictionary>
							</property>
						</object>
						<!--###################################-->
						<!--Data Process-->
						<!--###################################-->
						<object type="Oragon.CodeGen.Templating.T4Template, Oragon.CodeGen.Templating">
							<property name="TemplatePath" value="..\Template.DataProcess.tt"/>
							<property name="OutputPath" value="..\..\iMusica.Enterprise.Data.iMusicaImportacaoGenerica\Process\[DataProcess].CodeGen.cs"/>
							<property name="TemplateParameters">
								<dictionary key-type="System.String" value-type="System.Object">
									<entry key="NameSpace" value-ref="BaseNamespace" />
									<entry key="MyMetaDomainConverterWrapper" value-ref="MyMetaDomainConverterWrapper" />
								</dictionary>
							</property>
						</object>
						<!--###################################-->
						<!--Mappings-->
						<!--###################################-->
						<object type="Oragon.CodeGen.Templating.T4Template, Oragon.CodeGen.Templating">
							<property name="TemplatePath" value="..\Template.Mappings.tt"/>
							<property name="OutputPath" value="..\..\iMusica.Enterprise.Data.iMusicaImportacaoGenerica\Mapping\[FluentNhibernateMappings].CodeGen.cs"/>
							<property name="TemplateParameters">
								<dictionary key-type="System.String" value-type="System.Object">
									<entry key="NameSpace" value-ref="BaseNamespace" />
									<entry key="MyMetaDomainConverterWrapper" value-ref="MyMetaDomainConverterWrapper" />
								</dictionary>
							</property>
						</object>
						<!--###################################-->
					</list>
				</property>
			</object>

			<object name="MetadataAnalyser" type="Oragon.CodeGen.MetaData.MetadataAnalyser, Oragon.CodeGen.MetaData.Domain">
				<property name="IgnorePaterns">
					<list element-type="System.String">
						<value>mtFonogramaAgente</value>
						<value>mtAgente</value>
						<value>mtPaisISOWarnerDSPCode</value>
						<value>sonyGenreSubgenreMetaData</value>
						<value>sonyParticipant</value>
						<value>sonyTrackBundle</value>
						<value>sonyRetailer</value>
						<value>sonyPricing</value>
					</list>
				</property>
				<property name="FilterPaterns">
					<list element-type="System.String">
					</list>
				</property>
			</object>

		</objects>
	</spring>

	<system.runtime.remoting>
	</system.runtime.remoting>
	<startup useLegacyV2RuntimeActivationPolicy="true">
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
	</startup>
</configuration>
